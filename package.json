{
  "name": "react-docgen-imported-proptypes-handler",
  "version": "1.0.0",
  "description": "evaluate variables from external files for react-docgen",
  "scripts": {
    "build": "rimraf dist/ && babel src/ --out-dir dist/ --ignore **/__tests__,**/__mocks__,**/src/types.js",
    "lint": "eslint . --report-unused-disable-directives",
    "lint-file": "eslint --report-unused-disable-directives",
    "fix-lint": "npm run lint -- --fix",
    "test": "jest"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benjroy/react-docgen-imported-proptypes-handler.git"
  },
  "keywords": [],
  "author": "benjroy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/benjroy/react-docgen-imported-proptypes-handler/issues"
  },
  "homepage": "https://github.com/benjroy/react-docgen-imported-proptypes-handler#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/runtime": "^7.2.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.9.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.0",
    "glob": "^7.1.3",
    "jest": "^23.6.0",
    "prettier": "^1.15.3",
    "react-router": "^4.3.1",
    "rimraf": "^2.6.2"
  },
  "jest": {
    "roots": [
      "src"
    ],
    "testRegex": "/__tests__/.*-test\\.js$"
  },
  "dependencies": {
    "react-docgen": "2.21.0",
    "recast": "^0.12.9"
  }
}
